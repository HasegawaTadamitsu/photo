= form_for @upload_file , :html => { :multipart => true }do |f|
  -if @upload_file.errors.any?
    #error_explanation
      %h2= "#{pluralize(@upload_file.errors.count, "error")} prohibited this upload_file from being saved:"
      %ul
        - @upload_file.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :upload_file_name
    = f.file_field :upload_file_name
  .actions
    = f.submit 'upload'
